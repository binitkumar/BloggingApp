:css  .full_length{    padding: 2px 100px;    border: 1px solid black;    margin-bottom: 2px;  }  .without_symbol{    list-style: none outside none;  }  .blog_links{    margin-top: 5px;  }  .header{    font-size: 18px;    font-weight: bold;  }%ul.without_symbol  %li.header List of latest Ten blogs  - BlogEntry.all.each do |blog|    %li.blog_links      %a.full_length{:href=>"/blog_entries/#{blog.id}"}        = blog.subject